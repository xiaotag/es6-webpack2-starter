{
  "name": "es6-webpack2-starter",
  "version": "2.0.1",
  "description": "A template project for es6/7, webpack2, sass and postcss",
  "scripts": {
    "postinstall": "cp env.example.js env.js",
    "start": "webpack-dev-server --config webpack/dev.config.js --watch",
    "build": "NODE_ENV=production webpack --config webpack/prod.config.js --progress --display-error-details --color",
    "precommit": "eslint src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/micooz/es6-webpack-starter.git"
  },
  "keywords": [
    "es6",
    "es7",
    "webpack2",
    "starter",
    "template",
    "postcss",
    "sass",
    "css module"
  ],
  "author": "Micooz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/micooz/es6-webpack-starter/issues"
  },
  "homepage": "https://github.com/micooz/es6-webpack-starter#readme",
  "dependencies": {
    "babel-polyfill": "^6.20.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.6.0",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-class-properties": "^6.19.0",
    "babel-plugin-transform-object-rest-spread": "^6.20.2",
    "babel-preset-latest": "^6.16.0",
    "clean-webpack-plugin": "^0.1.14",
    "css-loader": "^0.26.1",
    "eslint": "^3.12.2",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-react": "^6.8.0",
    "extract-text-webpack-plugin": "^2.0.0-beta.3",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.24.1",
    "json-loader": "^0.5.4",
    "node-sass": "^4.1.1",
    "postcss-loader": "^1.2.1",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.1.0-beta.20",
    "webpack-dev-server": "^2.1.0-beta.0",
    "webpack-merge": "^2.0.0"
  }
}
